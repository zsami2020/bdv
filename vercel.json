{
    "functions": {
      "api/api.php": {
        "runtime": "vercel-php@0.7.3"
      }
    },
    "routes": [
      {
        "src": "/api/api.php",
        "dest": "/api/api.php"
      },
      {
        "src": "/assets/styles/style.css",
        "dest": "/public/assets/styles/style.css"
      },
      {
        "src": "/assets/scripts/script.js",
        "dest": "/public/assets/scripts/script.js"
      },
      {
        "src": "/(.*)",
        "dest": "/api/api.php"
      }
    ]
  }
